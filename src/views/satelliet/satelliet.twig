{% extends 'layout.twig' %}

{% set pageTitle= 'Fcom - Satelliet info' %}

{% block pageContent %}
    <article>
        {% for satelliet in satellieten %}
            <article
                <p><a href="{{ app.request.baseUrl }}/satelliet/{{ satelliet.satellietenId }}"> {{ satelliet.titel }}</a></p>
            </article>
        {% endfor %}
    </article>
{% endblock %}